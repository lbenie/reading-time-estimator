    },
    "./i18n/it": {
      "types": "./dist/types/i18n/it.d.ts",
      "import": "./dist/i18n/it/locale.js",
      "require": "./dist/i18n/it/locale.cjs"
    },
    "./i18n/id": {
      "types": "./dist/types/i18n/id.d.ts",
      "import": "./dist/i18n/id/locale.js",
      "require": "./dist/i18n/id/locale.cjs"
    },
    "./package.json": "./package.json"
  },
  "bugs": {
    "url": "https://github.com/lbenie/reading-time-estimator/issues"
  },
  "homepage": "https://github.com/lbenie/reading-time-estimator#readme",
  "author": {
    "name": "Lucien Bénié",
    "email": "lucien.benie@gmail.com",
    "url": "https://github.com/lbenie"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lbenie/reading-time-estimator.git"
